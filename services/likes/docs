POST /post/like
PostID uint `binding:"required"`
UserID uint `binding:"required"`
return: total int

POST /post/unlike
PostID uint `binding:"required"`
UserID uint `binding:"required"`
return: total int

POST /comment/like
CommentID uint `binding:"required"`
UserID uint `binding:"required"`
return: total int

POST /comment/unlike
CommentID uint `binding:"required"`
UserID uint `binding:"required"`
return: total int

GET /post/likes
IDs []uint
return: []{ID uint, Likes int}

GET /comment/likes
IDs []uint
return: []{ID uint, Likes int}

POST /post/haslike
IDs []uint
UserID uint `binding:"required"`
return: []{
    ID uint
	HasLike bool
}

POST /comment/haslike
IDs []uint
UserID uint `binding:"required"`
return: []{
    ID uint
	HasLike bool
}