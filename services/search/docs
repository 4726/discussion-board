POST /index
Title     string `binding:"required"`
Body      string `binding:"required"`
UserID    uint   `binding:"required"`
Id        uint `binding:"required"`
Timestamp int64
Likes     int
return: {}

GET /search
Term  string `form:"term" binding:"required"`
From  uint   `form:"from"`
Total uint   `form:"total" binding:"required"`
return: []uint (array of PostIDs)

POST /update/likes
Id    uint `binding:"required"`
Likes int
return: {}

POST /deletepost
Id uint `binding:"required"`
return: {}

POST /update/lastupdate
Id         uint `binding:"required"`
LastUpdate int64
return: {}